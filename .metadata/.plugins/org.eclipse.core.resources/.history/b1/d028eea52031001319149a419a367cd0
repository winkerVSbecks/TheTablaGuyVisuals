import processing.core.PApplet;
import themidibus.*;

public class Song {
	
	int t;
	int[] range;
	Note[] notes;
	MainApp p;
	
	
	public Song(MainApp _p, Note[] _notes, int[] _range, int _t) {
		p = _p;
		notes = _notes;
		range = _range;
		t = _t;
	}

	
	public void draw() {
		for (int i = 0; i < range.length; i++) {
			int noteToCheck = range[i];
			if( notes[range[i]].isOn ) {
				MainApp.println("note on: "+ noteToCheck);
			}
		}
	}
	

}
