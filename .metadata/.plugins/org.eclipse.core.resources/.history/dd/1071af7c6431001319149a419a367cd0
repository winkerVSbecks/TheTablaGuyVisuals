import processing.core.PApplet;

public class Song {
	
	int t;
	int[] range;
	Note[] notes;
	PApplet p;
	
	public Song(PApplet _p, Note[] _notes, int[] _range, int _t) {
		p = _p;
		notes = _notes;
		range = _range;
		t = _t;
	}
	
	public void draw() {
		for (int i = 0; i < range.length; i++) {
			int noteToCheck = range[i];
		}
	}
	

}
